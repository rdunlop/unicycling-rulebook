%h1 Listing votes
%table
  %tr
    %th User
    %th Vote
    %th
    %th
    %th
  - @votes.each do |vote|
    %tr
      %td= vote.user
      %td= vote.vote
      %td= link_to 'Edit', edit_proposal_vote_path(@proposal, vote)
